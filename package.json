{
  "name": "caste-portfolio",
  "version": "1.0.0",
  "description": "Diego Castellanos Portfolio",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss assets/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss assets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' assets/css/style.comp.css -o assets/css/style.prefix.css",
    "compress:css": "node-sass assets/css/style.prefix.css assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Castelatzi/caste-portfolio.git"
  },
  "author": "Diego Castellanos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Castelatzi/caste-portfolio/issues"
  },
  "homepage": "https://github.com/Castelatzi/caste-portfolio#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0"
  }
}
